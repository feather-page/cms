<tr id="<%= dom_id(page) %>" is="turbo-frame">
  <td><%= page.title %></td>
  <td><%= page.slug %></td>
  <td>
    <%= link_to 'Edit', edit_page_path(page), class: 'button is-small' %>
    <%= link_to 'Delete', page_path(page), data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' }, class: 'button is-small' %>
  </td>
</tr>
